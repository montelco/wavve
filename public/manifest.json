{
  "short_name": "Wavvve",
  "name": "Wavvve",
  "manifest_version": 1,
  "version": "Cascade",
  "default_locale": "en",
  "description": "Wavvve is a marketing platform for businesses, designed to make engaging customers easier.",
  "incognito": "not_allowed",
  "permissions": [
    "bookmarks",
    "browsingData",
    "clipboardRead",
    "clipboardWrite",
    "cookies",
    "gcm",
    "geolocation",
    "history",
    "identity",
    "notificiations"
  ],
  "icons": [
    {
      "src": "favicons/android-chrome-96x96.png",
      "sizes": "96x96",
      "type": "image/png"
    },
    {
      "src": "favicons/android-chrome-144x144.png",
      "sizes": "144x144",
      "type": "image/png"
    },
    {
      "src": "favicons/android-chrome-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    }
  ],
  "start_url": "/",
  "display": "standalone",
  "background_color": "#cbd2d6",
  "theme_color": "#415d6c"
}